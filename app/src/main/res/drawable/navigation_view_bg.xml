<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="rectangle">
            <solid android:color="#E9F3F5" />
        </shape>
    </item>

    <item
        android:left="-2dp"
        android:right="-2dp"
        android:top="-2dp"
        android:bottom="1dp">
        <shape>
            <solid android:color="#E9F3F5" />
            <stroke
                android:width="1dp"
                android:color="#A8A7A9" />
        </shape>
    </item>

</layer-list>